#%RAML 1.0

title: Discriminator

# https://github.com/raml-org/raml-java-parser/issues/139

types:
  Person:
    type: object
    properties:
      name: string
  Employee:
    type: Person
    properties:
      employeeId: string
  User:
    type: Person
    properties:
      userId: string

  UserOrEmplo:
   type: User | Employee
   discriminator: isClever
