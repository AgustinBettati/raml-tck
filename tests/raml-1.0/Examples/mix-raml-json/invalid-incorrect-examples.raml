#%RAML 1.0
title: test

types:
  JsonRaml:
    type: |
      {
        "type": "object",
        "$schema": "http://json-schema.org/draft-04/schema",
        "required": ["country_name", "population"],
        "properties": {
          "country_name": { "type": "string" },
          "population": { "type": "number" }
        }
      }
    example:
      country_name: 11.1
      population: Somewhere
  RamlJson:
    type: object
    properties:
      country_name: string
      population: number
    example: |
      {
        "country_name": 11.2,
        "population": "Somewhere"
      }

